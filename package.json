{
  "name": "ci-cd",
  "private": true,
  "version": "0.0.0",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "lint": "eslint src --max-warnings=0",
    "prepare": "husky install",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "vite": "^5.0.8",
    "vitest": "1.6.1",
    "husky": "8.0.0",
    "release-it": "^15.0.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true,
      "requireBranch": "main",
      "tagName": "v${version}",
      "commitMessage": "chore(release): v${version}",
      "tagAnnotation": "Release v${version}"
    }
  }
}
